{
  "name": "@arnaud-barre/eslint-plugin-dev",
  "private": true,
  "version": "1.2.6",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tnode build.ts",
    "test": "tnode tests/index.ts",
    "lint": "yarn lint-ci --fix --cache",
    "lint-ci": "eslint ./ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "ci": "tsc && yarn test && yarn lint-ci && yarn build",
    "publish": "yarn ci && cd dist && npm publish"
  },
  "dependencies": {
    "@eslint-community/regexpp": "^4.6.1",
    "@typescript-eslint/utils": "^6.7.2"
  },
  "devDependencies": {
    "@arnaud-barre/eslint-config": "^3.1.2",
    "@arnaud-barre/tnode": "^0.19.1",
    "@types/node": "^18.17.19",
    "@typescript-eslint/rule-tester": "^6.7.2",
    "eslint": "^8.50.0",
    "typescript": "^5.2.2"
  }
}
